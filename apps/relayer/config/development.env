# Development Environment Configuration
NODE_ENV=development

# Stacks Configuration
STACKS_NETWORK=testnet
STACKS_API_URL=https://api.testnet.hiro.so
STACKS_CONTRACT_ADDRESS=SP[DEV-CONTRACT].brick-vault-gateway
STACKS_PRIVATE_KEY=0x[DEV-STACKS-KEY]
STACKS_FEE_RATE=0.000001

# EVM Configuration
EVM_NETWORK=sepolia
EVM_RPC_URL=https://sepolia.infura.io/v3/[DEV-INFURA-KEY]
EVM_STACKS_MANAGER_ADDRESS=0x[DEV-MANAGER-ADDRESS]
EVM_PRIVATE_KEY=0x[DEV-EVM-KEY]
EVM_GAS_LIMIT=500000

# Monitoring Configuration
MONITORING_INTERVAL=5000
MONITORING_MAX_RETRIES=3
MONITORING_RETRY_DELAY=2000
MONITORING_BATCH_SIZE=10
MONITORING_TIMEOUT=30000

# Logging Configuration
LOG_LEVEL=debug
LOG_ENABLE_CONSOLE=true
LOG_ENABLE_FILE=false
